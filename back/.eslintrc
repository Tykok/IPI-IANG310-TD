{
  "env": {
    "browser": false,
    "es2021": true,
    "node": true
    // "jest/globals": true
  },
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "airbnb-base",
    "airbnb-typescript",
    "prettier"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "tsconfig.json"
  },
  "rules": {
    "quotes": "off",
    "@typescript-eslint/quotes": "warn",
    "semi": ["error", "never"],
    "@typescript-eslint/semi": "off"
  },
  "ignorePatterns": [
    "**/*.js"
  ]
}