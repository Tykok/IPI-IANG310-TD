{
  "name": "iang300-td",
  "version": "1.0.0",
  "description": "A simple API of Football Club build with NodeJS, Express, Typescript and Sequelize",
  "main": "dist/index.js",
  "author": "Tykok <treportelie12@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"yarn build:watch\" \"yarn dev:nodemon\"",
    "dev:nodemon": "nodemon dist/index.js",
    "build": "npx tsc",
    "build:watch": "yarn build -w",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "gen:secret": "openssl rand -base64 32 > ./secret.key"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "dotenv": "^14.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "concurrently": "^7.5.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "sequelize-cli": "^6.4.1",
    "typescript": "*"
  },
  "engines": {
    "node": ">=16"
  }
}
